{
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/saturn-assets/(.*)", "destination": "/saturn-assets/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/*": {
      "runtime": "nodejs20.x"
    }
  }
}
