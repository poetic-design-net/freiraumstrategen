{
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/saturn-assets/(.*)", "destination": "/saturn-assets/$1" },
    { "source": "/", "destination": "/summer-academy" },
    { "source": "/((?!summer-academy|assets|saturn-assets).*)", "destination": "/summer-academy" }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/((?!_next/static|assets|saturn-assets).*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ]
}
