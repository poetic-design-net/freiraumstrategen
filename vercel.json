{
  "buildCommand": "pnpm run build",
  "outputDirectory": ".",
  "builds": [
    { "src": "studio/package.json", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "build" } }
  ],
  "routes": [
    { "src": "/studio/(.*)", "dest": "/studio/$1" },
    { "src": "/(.*)", "dest": "/build/$1" }
  ]
}
