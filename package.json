{
  "name": "sanity-template-sveltekit-clean",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm run build:studio && pnpm run build:app",
    "build:studio": "pnpm --filter studio build",
    "build:app": "pnpm --filter app build",
    "start": "pnpm --filter app start",
    "format": "prettier --cache --write ."
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson"
    ],
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "prettier": "3.3.3",
    "prettier-plugin-packagejson": "2.5.1"
  },
  "packageManager": "pnpm@8.15.4"
}
